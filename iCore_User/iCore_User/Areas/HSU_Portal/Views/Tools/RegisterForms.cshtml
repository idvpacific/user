@{
    ViewBag.Title = "Tools - Registration Forms";
    Layout = "~/Areas/HSU_Portal/Views/Shared/_LayoutPortal.cshtml";
}
@section CSSStyle {
    <link rel="stylesheet" type="text/css" href="~/Theme/Base/app-assets/vendors/css/tables/datatable/datatables.min.css" />
    <link rel="stylesheet" type="text/css" href="~/Theme/Base/app-assets/vendors/css/extensions/sweetalert2.min.css" />
    <link rel="stylesheet" type="text/css" href="~/Theme/Base/app-assets/vendors/css/forms/select/select2.min.css" />
    <link rel="stylesheet" href="~/Theme/Base/assets/dropify/dist/css/dropify.min.css" />
    <style>
        table.dataTable thead .sorting:before, table.dataTable thead .sorting_asc:before, table.dataTable thead .sorting_desc:before, table.dataTable thead .sorting:after, table.dataTable thead .sorting_asc:after, table.dataTable thead .sorting_desc:after {
            left: 10px
        }

        .MenuToolbox:after {
            content: '\2807';
            font-size: 22px;
        }

        tr:hover {
            background-color: rgba(235, 234, 234, 0.76) !important;
        }

        .lds-ripple {
            display: inline-block;
            position: relative;
            width: 80px;
            height: 80px;
        }

            .lds-ripple div {
                position: absolute;
                border: 4px solid #006abc;
                opacity: 1;
                border-radius: 50%;
                animation: lds-ripple 1s cubic-bezier(0, 0.2, 0.8, 1) infinite;
            }

                .lds-ripple div:nth-child(2) {
                    animation-delay: -0.5s;
                }

        @@keyframes lds-ripple {
            0% {
                top: 36px;
                left: 36px;
                width: 0;
                height: 0;
                opacity: 1;
            }

            100% {
                top: 0px;
                left: 0px;
                width: 72px;
                height: 72px;
                opacity: 0;
            }
        }
    </style>
}
<section id="Sec_FormDefine" style="pointer-events:auto;opacity:1;display:block">
    <div class="row">
        <div class="col-12">
            <div class="card">
                <div class="card-header">
                    <div class="content-header-left col-md-9 col-12">
                        <div class="row breadcrumbs-top">
                            <div class="col-12" style="padding-left:10px">
                                <h2 class="content-header-title float-left mb-0">Tools</h2>
                                <div class="breadcrumb-wrapper col-12">
                                    <ol class="breadcrumb" style="pointer-events:auto;opacity:1">
                                        <li class="breadcrumb-item active">Registration Forms Management</li>
                                    </ol>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="card-content">
                    <div class="card-body card-dashboard" style="padding-bottom:35px;">
                        <div class="form-inline" style="width:100%;padding-bottom:20px" id="Sec_Form">
                            <div class="col-lg-9">
                                <div class="text-bold-300 font-medium-1" style="margin-top:10px;margin-left:5px">
                                    Form Name :
                                </div>
                                <fieldset class="form-group position-relative has-icon-left input-divider-left" style="width:100%">
                                    <input type="text" class="form-control" id="TXT_Form_Name" placeholder="" style="width:100%" maxlength="200">
                                    <div class="form-control-position">
                                        <i class="fa fa-list-alt"></i>
                                    </div>
                                </fieldset>
                            </div>
                            <div class="col-lg-3">
                                <div class="text-bold-300 font-medium-1" style="margin-top:10px;margin-left:5px">
                                    Template :
                                </div>
                                <select class="select2 form-control" id="TXT_Form_Template">
                                    <option value="1">Vuexy Version 1</option>
                                    <option value="2" selected>Vuexy Version 2</option>
                                    <option value="3">Ocean</option>
                                </select>
                            </div>
                            <div class="col-lg-12">
                                <div class="text-bold-300 font-medium-1" style="margin-top:10px;margin-left:5px">
                                    Decription :
                                </div>
                                <fieldset class="form-group position-relative has-icon-left input-divider-left" style="width:100%">
                                    <input type="text" class="form-control" id="TXT_Form_Description" placeholder="" style="width:100%" maxlength="200">
                                    <div class="form-control-position">
                                        <i class="fa fa-sticky-note"></i>
                                    </div>
                                </fieldset>
                            </div>
                            <div class="col-lg-12" style="text-align:right">
                                <div style="margin-top:20px;display:none" id="Btn_Form_Wait">
                                    <button class="btn btn-primary mb-1" type="button" disabled>
                                        <span class="spinner-border spinner-border-sm" role="status" aria-hidden="true" style="margin-right:10px"></span>
                                        Please wait ...
                                    </button>
                                </div>
                                <div class="btn-group" role="group" style="margin-top:20px;display:block" id="Btn_Form">
                                    <button type="button" class="btn btn-primary" onclick="Form_AddNew()"><i class="fa fa-plus" style="margin-right:10px"></i>Add form</button>
                                    <button type="button" class="btn btn-success" onclick="ReloadGrid()"><i class="fa fa-refresh" style="margin-right:10px"></i>Reload</button>
                                    <button type="button" class="btn btn-danger" onclick="Form_Clear()"><i class="fa fa-trash" style="margin-right:10px"></i>Clear</button>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-12" id="Sec_TableTag_Wait" style="display:none;text-align:center;padding:20px">
                            <div class="lds-ripple"><div></div><div></div></div>
                        </div>
                        <div class="table-responsive" style="pointer-events:auto;opacity:1;display:block" id="Sec_TableTag">
                            <table id="ToolsManagementTable" class="table table-striped table-bordered">
                                <thead>
                                    <tr>
                                        <th style="width:30px;"></th>
                                        <th style="padding-left: 30px">Name</th>
                                        <th style="padding-left: 30px">Description</th>
                                        <th style="padding-left: 30px">Template</th>
                                        <th style="width:75px;padding-left:30px">Status</th>
                                        <th style="width:230px"></th>
                                        <th style="width:20px"></th>
                                    </tr>
                                </thead>
                                <tbody id="GridBody">
                                    @if (ViewBag.DT != null)
                                    {
                                        foreach (System.Data.DataRow RW in ViewBag.DT)
                                        {
                                            <tr>
                                                <td style="text-align:center;"><i class="fa fa-list-alt text-primary" style="font-size:25px"></i></td>
                                                <td>@RW[2].ToString().Trim()</td>
                                                <td>@RW[3].ToString().Trim()</td>
                                                <td>@RW[4].ToString().Trim()</td>
                                                @if (RW[5].ToString().Trim() == "0")
                                                {
                                                    <td style="text-align:center">
                                                        <div class="badge badge-pill badge-light-danger" style="width:70px">Disabled</div>
                                                    </td>
                                                }
                                                else
                                                {
                                                    <td style="text-align:center">
                                                        <div class="badge badge-pill badge-light-success" style="width:70px">Active</div>
                                                    </td>
                                                }
                                                <td style="text-align:center">
                                                    <div class="btn btn-info" style="font-size:12px" onclick="Show_Designform('@RW[0].ToString().Trim()','@RW[2].ToString().Trim()')">
                                                        <i class="fa fa-paint-brush" style="margin-right:5px;margin-left:5px"></i>
                                                        Form designer
                                                    </div>
                                                </td>
                                                <td style="text-align:center">
                                                    <div class="btn-group dropleft">
                                                        <button type="button" class="btn" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                                            <div class="MenuToolbox text-primary" />
                                                        </button>
                                                        <div class="dropdown-menu" style="font-size:12px">
                                                            <a class="dropdown-item" href="javascript:void(0)" onclick="Form_Remove('@RW[0].ToString().Trim()','@RW[2].ToString().Trim()')"><i class="fa fa-trash-o text-primary" style="width:24px;font-size:14px"></i>Remove form</a>
                                                            <a class="dropdown-item" href="javascript:void(0)" onclick="Form_ChangeStatus('@RW[0].ToString().Trim()','@RW[2].ToString().Trim()')"><i class="fa fa-refresh text-primary" style="width:24px;font-size: 14px"></i>Change status</a>
                                                            <a class="dropdown-item" href="javascript:void(0)" onclick="Form_Edit('@RW[0].ToString().Trim()','@RW[2].ToString().Trim()')"><i class="fa fa-pencil-square-o text-primary" style="width:24px;font-size: 14px"></i>Edit form</a>
                                                            <div class="dropdown-divider"></div>
                                                            <a class="dropdown-item" href="javascript:void(0)" onclick="Form_ConfigurationST('@RW[1].ToString().Trim()')"><i class="fa fa-cogs text-primary" style="width:24px;font-size: 14px"></i>Form configuration</a>
                                                            <div class="dropdown-menu" style="font-size:12px">
                                                                <a class="dropdown-item" href="javascript:void(0)" onclick="Form_CopyLink('1', '@RW[0].ToString().Trim()')"><i class="fa fa-copy text-primary" style="width:24px;font-size: 14px"></i> Copy Customer URL link </a>
                                                                <a class="dropdown-item" href="javascript:void(0)" onclick="Form_CopyLink('2', '@RW[0].ToString().Trim()')"><i class="fa fa-copy text-primary" style="width:24px;font-size: 14px"></i> Copy Customer iFrame URL </a>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </td>
                                            </tr>
                                        }
                                    }
                                </tbody>
                                <tfoot>
                                    <tr>
                                        <th style="width:30px;"></th>
                                        <th style="padding-left: 30px">Name</th>
                                        <th style="padding-left: 30px">Description</th>
                                        <th style="padding-left: 30px">Template</th>
                                        <th style="width:75px;padding-left:30px">Status</th>
                                        <th style="width:230px"></th>
                                        <th style="width:20px"></th>
                                    </tr>
                                </tfoot>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<div class="modal fade text-left" id="Modal_FormEditor" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered modal-dialog-scrollable" role="document">
        <div class="modal-content">
            <div class="modal-header bg-primary white" style="border-radius:7px 7px 0px 0px">
                <h5 class="modal-title" id="Modal_FormEditor_Title"></h5>
            </div>
            <div class="modal-body">
                <div class="col-lg-12" id="Modal_FormEditor_Body">
                    <div class="col-lg-12">
                        <div class="text-bold-300 font-medium-1" style="margin-top:10px;margin-left:5px">
                            Form Name :
                        </div>
                        <fieldset class="form-group position-relative has-icon-left input-divider-left" style="width:100%">
                            <input type="text" class="form-control" id="M_TXT_Form_Name" placeholder="" style="width:100%" maxlength="200">
                            <div class="form-control-position">
                                <i class="fa fa-list-alt"></i>
                            </div>
                        </fieldset>
                    </div>
                    <div class="col-lg-12">
                        <div class="text-bold-300 font-medium-1" style="margin-top:10px;margin-left:5px">
                            Template :
                        </div>
                        <select class="select2 form-control" id="M_TXT_Form_Template">
                            <option value="1" selected>Vuexy Version 1</option>
                            <option value="2" selected>Vuexy Version 2</option>
                            <option value="3" selected>Ocean</option>
                        </select>
                    </div>
                    <div class="col-lg-12">
                        <div class="text-bold-300 font-medium-1" style="margin-top:10px;margin-left:5px">
                            Decription :
                        </div>
                        <fieldset class="form-group position-relative has-icon-left input-divider-left" style="width:100%">
                            <input type="text" class="form-control" id="M_TXT_Form_Description" placeholder="" style="width:100%" maxlength="200">
                            <div class="form-control-position">
                                <i class="fa fa-sticky-note"></i>
                            </div>
                        </fieldset>
                    </div>
                    <div class="col-lg-12">
                        <div class="text-bold-300 font-medium-1" style="margin-top:10px;margin-left:5px">
                            Customer form URL :
                        </div>
                        <fieldset class="form-group position-relative has-icon-left input-divider-left" style="width:100%">
                            <input type="text" class="form-control" id="M_TXT_Form_CSURL" placeholder="" style="width:100%" maxlength="200" readonly="readonly">
                            <div class="form-control-position">
                                <i class="fa fa-link"></i>
                            </div>
                        </fieldset>
                    </div>
                    <div class="col-lg-12" style="text-align:right">
                        <button type="button" onclick="Form_GenerateURL('1')" class="btn btn-success">New URL</button>
                        <button type="button" onclick="Form_Copy('1')" class="btn btn-info">Copy</button>
                    </div>
                    <div class="col-lg-12">
                        <div class="text-bold-300 font-medium-1" style="margin-top:10px;margin-left:5px">
                            iFrame Customer form URL :
                        </div>
                        <fieldset class="form-group position-relative has-icon-left input-divider-left" style="width:100%">
                            <input type="text" class="form-control" id="M_TXT_Form_iFrameURL" placeholder="" style="width:100%" maxlength="200" readonly="readonly">
                            <div class="form-control-position">
                                <i class="fa fa-external-link"></i>
                            </div>
                        </fieldset>
                    </div>
                    <div class="col-lg-12" style="text-align:right">
                        <button type="button" onclick="Form_GenerateURL('2')" class="btn btn-success">New URL</button>
                        <button type="button" onclick="Form_Copy('2')" class="btn btn-info">Copy</button>
                    </div>
                </div>
                <div class="col-lg-12" id="Modal_FormEditor_Wait" style="display:none;text-align:center;padding:10px">
                    <div class="lds-ripple"><div></div><div></div></div>
                </div>
                <input type="hidden" id="Modal_FormID" value="" />
                <input type="hidden" id="Modal_FormURL1" value="" />
                <input type="hidden" id="Modal_FormURL2" value="" />
            </div>
            <div class="modal-footer" id="Modal_FormEditor_Btn" style="opacity:1;pointer-events:auto">
                <button type="button" class="btn btn-primary" onclick="Form_Edit_Save()">Save changes</button>
                <button type="button" class="btn btn-danger" data-dismiss="modal">Cancel</button>
            </div>
        </div>
    </div>
</div>
<section id="Sec_Section" style="pointer-events:auto;opacity:1;display:none">
    <div class="row">
        <div class="col-12">
            <div class="card">
                <div class="card-header">
                    <div class="content-header-left col-md-9 col-12">
                        <div class="row breadcrumbs-top">
                            <div class="col-12" style="padding-left:10px">
                                <h2 class="content-header-title float-left mb-0">Form <span id="Sec_Section_Groupname">#</span></h2>
                                <div class="breadcrumb-wrapper col-12">
                                    <ol class="breadcrumb" style="pointer-events:auto;opacity:1">
                                        <li class="breadcrumb-item active">Form registration section designer</li>
                                    </ol>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="card-content">
                    <div class="card-body card-dashboard" style="padding-bottom:35px;">
                        <div class="col-lg-12 form-inline" id="Sec_Section_Management" style="pointer-events:auto;opacity:1;margin-bottom:20px">
                            <div class="col-lg-5">
                                <div class="text-bold-300 font-medium-1" style="margin-top:10px;margin-left:5px">
                                    Section Name :
                                </div>
                                <fieldset class="form-group position-relative has-icon-left input-divider-left" style="width:100%">
                                    <input type="text" class="form-control" id="Txt_Section_Name" placeholder="" style="width:100%" maxlength="200">
                                    <div class="form-control-position">
                                        <i class="fa fa-tag"></i>
                                    </div>
                                </fieldset>
                            </div>
                            <div class="col-lg-3">
                                <div class="text-bold-300 font-medium-1" style="margin-top:10px;margin-left:5px">
                                    Icon code : ( <a href="@Url.Action("IconPack","Dashboard", new { id="", area="HSU_Portal" })" target="_blank">Click here to see all icons</a> )
                                </div>
                                <fieldset class="form-group position-relative has-icon-left input-divider-left" style="width:100%">
                                    <input type="text" class="form-control" id="Txt_Section_Icon" placeholder="" style="width:100%" maxlength="200">
                                    <div class="form-control-position">
                                        <i class="fa fa-link"></i>
                                    </div>
                                </fieldset>
                            </div>
                            <div class="col-lg-2">
                                <div class="text-bold-300 font-medium-1" style="margin-top:10px;margin-left:5px">
                                    Section order No. :
                                </div>
                                <fieldset class="form-group position-relative has-icon-left input-divider-left" style="width:100%">
                                    <input type="text" class="form-control" id="Txt_Section_Index" oninput="this.value = this.value.replace(/[^0-9]/g, '').replace(/(\..*)\./g, '$1');" placeholder="" style="width:100%;text-align:center" maxlength="200">
                                    <div class="form-control-position">
                                        <i class="fa fa-list"></i>
                                    </div>
                                </fieldset>
                            </div>
                            <div class="col-lg-2">
                                <div class="text-bold-300 font-medium-1" style="margin-top:10px;margin-left:5px">
                                    Width :
                                </div>
                                <select class="select2 form-control" id="Txt_Section_Width">
                                    <option value="1">Col 1</option>
                                    <option value="2">Col 2</option>
                                    <option value="3">Col 3</option>
                                    <option value="4">Col 4</option>
                                    <option value="5">Col 5</option>
                                    <option value="6">Col 6</option>
                                    <option value="7">Col 7</option>
                                    <option value="8">Col 8</option>
                                    <option value="9">Col 9</option>
                                    <option value="10">Col 10</option>
                                    <option value="11">Col 11</option>
                                    <option value="12">Col 12</option>
                                </select>
                            </div>
                            <div class="col-lg-12" style="text-align:right">
                                <div style="margin-top:20px;display:none" id="Btn_Section_Wait">
                                    <button class="btn btn-primary" type="button" disabled>
                                        <span class="spinner-border spinner-border-sm" role="status" aria-hidden="true" style="margin-right:10px"></span>
                                        Please wait ...
                                    </button>
                                </div>
                                <div class="btn-group" role="group" style="margin-top:20px;display:block" id="Btn_Group_Section">
                                    <button type="button" class="btn btn-primary" onclick="AddNewSection()"><i class="fa fa-plus" style="margin-right:10px"></i>Add section</button>
                                    <button type="button" class="btn btn-danger" onclick="ClearSection()"><i class="fa fa-trash" style="margin-right:10px"></i>Clear</button>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-12" style="opacity:1;pointer-events:auto;" id="Btn_Controller_Sec">
                            <div class="btn btn-primary" style="font-size:12px;margin:3px" onclick="Back_To_Group()">
                                <i class="fa fa-step-backward" style="margin-right:5px;margin-left:5px"></i>
                                Go to select a form
                            </div>
                            <div class="btn btn-outline-success" style="font-size:12px;margin:3px" onclick="ReloadGrid_Section()">
                                <i class="fa fa-refresh" style="margin-right:5px;margin-left:5px"></i>
                                Reload
                            </div>
                            <div class="btn btn-outline-info" style="font-size:12px;margin:3px" onclick="Preview_All()">
                                <i class="fa fa-search" style="margin-right:5px;margin-left:5px"></i>
                                Form preview
                            </div>
                        </div>
                        <div class="col-lg-12" id="Sec_Section_Wait" style="display:none;text-align:center;padding:20px">
                            <div class="lds-ripple"><div></div><div></div></div>
                        </div>
                        <div class="table-responsive" style="pointer-events:auto;opacity:1;display:block" id="Sec_Section_Table">
                            <table id="SectionTable" class="table table-striped table-bordered">
                                <thead>
                                    <tr>
                                        <th style="padding-left:30px">Name</th>
                                        <th style="width:50px;padding-left:30px">Icon</th>
                                        <th style="width:80px;padding-left:30px">Order No.</th>
                                        <th style="width:80px;padding-left:30px">Width</th>
                                        <th style="width:120px;padding-left:30px">Status</th>
                                        <th style="width:20px"></th>
                                        <th style="width:220px"></th>
                                    </tr>
                                </thead>
                                <tbody id="GridBody_Section">
                                </tbody>
                                <tfoot>
                                    <tr>
                                        <th style="padding-left:30px">Name</th>
                                        <th style="width:50px;padding-left:30px">Icon</th>
                                        <th style="width:80px;padding-left:30px">Order No.</th>
                                        <th style="width:80px;padding-left:30px">Width</th>
                                        <th style="width:120px;padding-left:30px">Status</th>
                                        <th style="width:20px"></th>
                                        <th style="width:220px"></th>
                                    </tr>
                                </tfoot>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<section id="Sec_Elements" style="pointer-events:auto;opacity:1;display:none">
    <div class="row">
        <div class="col-12">
            <div class="card">
                <div class="card-header">
                    <div class="content-header-left col-md-9 col-12">
                        <div class="row breadcrumbs-top">
                            <div class="col-12" style="padding-left:10px">
                                <h2 class="content-header-title float-left mb-0"><span id="Sec_Elements_Groupname">#</span> - <span id="Sec_Elements_Sectionname">#</span></h2>
                                <div class="breadcrumb-wrapper col-12">
                                    <ol class="breadcrumb" style="pointer-events:auto;opacity:1">
                                        <li class="breadcrumb-item active">Form registration element designer</li>
                                    </ol>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-12" style="text-align:right">
                        <div class="btn btn-dark" style="font-size:12px;margin:3px" onclick="Back_To_Group()">
                            <i class="fa fa-step-backward" style="margin-right:5px;margin-left:5px"></i>
                            Go to form selection
                        </div>
                        <div class="btn btn-primary" style="font-size:12px;margin:3px" onclick="Back_To_Section()">
                            <i class="fa fa-step-backward" style="margin-right:5px;margin-left:5px"></i>
                            Go to section selection
                        </div>
                    </div>
                </div>
                <div class="card-content">
                    <div class="card-body card-dashboard" style="padding-bottom:35px;">
                        <div class="col-lg-12 form-inline" id="Sec_Elements_Management" style="pointer-events:auto;opacity:1;margin-bottom:20px">
                            <div style="width:100%;text-align:left;margin-bottom:10px;border-bottom:1px solid #006abc">
