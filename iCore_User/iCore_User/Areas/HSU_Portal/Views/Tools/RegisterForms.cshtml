                document.getElementById("MDiv_EPS_12").style.display = "none";
                document.getElementById("MDiv_EPS_13").style.display = "none";
                ClearModalDiv();
                $('#Modal_ElementEditor_Properties').modal('show');
                var formData = new FormData();
                formData.append("GID", SelGroup);
                formData.append("SID", SelSec);
                formData.append("EID", EID);
                $.ajax({
                    url: "@Url.Action("RegisterForms_RetrieveElement_Properties", "Tools")", type: "POST", data: formData, dataType: 'json', contentType: false, processData: false, async: true,
                    error: function () {
                        toastr.clear();
                        $('#Modal_ElementEditor_Properties').modal('hide');
                        toastr.error('An error occurred while processing your request', 'An error has occurred! [C02]', { "showMethod": "slideDown", "hideMethod": "slideUp", positionClass: 'toast-bottom-right', "progressBar": true, "closeButton": true, "timeOut": 8000 });
                    },
                    success: function (data)
                    {
                        var ErrorCode = ""; var ErrorResult = "";
                        $.each(data, function (i, item) { ErrorResult = item.Text; ErrorCode = item.Value; });
                        if (ErrorCode == "0")
                        {
                            var SepData = ErrorResult.split("#");
                            document.getElementById("Modal_ElementEditor_Properties_Title").innerText = "Edit Element properties [ " + SepData[0].trim() + " ]";
                            document.getElementById("P_Modal_Element_Type").value = SepData[1].trim();
