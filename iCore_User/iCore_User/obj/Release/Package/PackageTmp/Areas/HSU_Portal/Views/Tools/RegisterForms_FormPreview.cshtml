@using System.Data;
@{
    iCore_Administrator.Modules.SQL_Tranceiver Sq = new iCore_Administrator.Modules.SQL_Tranceiver();
    ViewBag.Title = "Form Preview [ " + ViewBag.DT_Group[0][0].ToString().Trim() + " ]";
    Layout = "~/Areas/HSU_Portal/Views/Shared/_LayoutPortal.cshtml";
}
@section CSSStyle {
    <link rel="stylesheet" type="text/css" href="~/Theme/Base/app-assets/vendors/css/extensions/sweetalert2.min.css" />
    <link rel="stylesheet" type="text/css" href="~/Theme/Base/app-assets/vendors/css/forms/select/select2.min.css" />
    <link rel="stylesheet" type="text/css" href="~/Theme/Base/assets/dropify/dist/css/dropify.min.css" />
    <link rel="stylesheet" type="text/css" href="~/Theme/Base/app-assets/vendors/css/extensions/plyr.css">
    <link rel="stylesheet" type="text/css" href="~/Theme/Base/app-assets/css/plugins/extensions/media-plyr.css">
    @{
        DataTable DT_HEAD = new DataTable();
        DT_HEAD = Sq.Get_DTable_TSQL(iCore_Administrator.Modules.DataBase_Selector.Administrator, "Select Head_01_File_Name,Head_01_File_Type,Head_02_File_Name,Head_02_File_Type,Head_03_File_Name,Head_03_File_Type,Head_04_File_Name,Head_04_File_Type,Head_05_File_Name,Head_05_File_Type,Head_06_File_Name,Head_06_File_Type,Head_Script From Users_07_Hospitality_SingleUser_RegisterForms_Configuration Where (Form_ID = '" + ViewBag.FormID + "')");
        if (DT_HEAD.Rows.Count == 1)
        {
            for (int i = 1; i < 7; i++)
            {
                try
                {
                    if (DT_HEAD.Rows[0][(i - 1) * 2].ToString().Trim() != "")
                    {
                        var MainPath = Server.MapPath("~/Drive/Hospitality/Rgfmcon/" + ViewBag.FormID + "/" + DT_HEAD.Rows[0][(i - 1) * 2].ToString().Trim() + "." + DT_HEAD.Rows[0][((i - 1) * 2) + 1].ToString().Trim());
                        if (System.IO.File.Exists(MainPath) == true)
                        {
                            switch (DT_HEAD.Rows[0][((i - 1) * 2) + 1].ToString().Trim().ToUpper())
                            {
                                case "CSS":
                                    {
                                        <link href="~/Drive/Hospitality/Rgfmcon/@ViewBag.FormID/@DT_HEAD.Rows[0][(i - 1) * 2].ToString().Trim().@DT_HEAD.Rows[0][((i - 1) * 2) + 1].ToString().Trim()" rel="stylesheet" />
                                        break;
                                    }
                                case "JS":
                                    {
                                        <script src="~/Drive/Hospitality/Rgfmcon/@ViewBag.FormID/@DT_HEAD.Rows[0][(i - 1) * 2].ToString().Trim().@DT_HEAD.Rows[0][((i - 1) * 2) + 1].ToString().Trim()"></script>
                                        break;
                                    }
                                case "PNG":
                                    {
                                        <img src="~/Drive/Hospitality/Rgfmcon/@ViewBag.FormID/@DT_HEAD.Rows[0][(i - 1) * 2].ToString().Trim().@DT_HEAD.Rows[0][((i - 1) * 2) + 1].ToString().Trim()" />
                                        break;
                                    }
                                case "JPG":
                                    {
                                        <img src="~/Drive/Hospitality/Rgfmcon/@ViewBag.FormID/@DT_HEAD.Rows[0][(i - 1) * 2].ToString().Trim().@DT_HEAD.Rows[0][((i - 1) * 2) + 1].ToString().Trim()" />
                                        break;
                                    }
                                case "JPEG":
                                    {
                                        <img src="~/Drive/Hospitality/Rgfmcon/@ViewBag.FormID/@DT_HEAD.Rows[0][(i - 1) * 2].ToString().Trim().@DT_HEAD.Rows[0][((i - 1) * 2) + 1].ToString().Trim()" />
                                        break;
                                    }
                                case "ICO":
                                    {
                                        <img src="~/Drive/Hospitality/Rgfmcon/@ViewBag.FormID/@DT_HEAD.Rows[0][(i - 1) * 2].ToString().Trim().@DT_HEAD.Rows[0][((i - 1) * 2) + 1].ToString().Trim()" />
                                        break;
                                    }
                                case "GIF":
                                    {
                                        <img src="~/Drive/Hospitality/Rgfmcon/@ViewBag.FormID/@DT_HEAD.Rows[0][(i - 1) * 2].ToString().Trim().@DT_HEAD.Rows[0][((i - 1) * 2) + 1].ToString().Trim()" />
                                        break;
                                    }
                                default:
                                    {
                                        <a href="~/Drive/Hospitality/Rgfmcon/@ViewBag.FormID/@DT_HEAD.Rows[0][(i - 1) * 2].ToString().Trim().@DT_HEAD.Rows[0][((i - 1) * 2) + 1].ToString().Trim()">@DT_HEAD.Rows[0][(i - 1) * 2].ToString().Trim().@DT_HEAD.Rows[0][((i - 1) * 2) + 1].ToString().Trim()</a>
                                        break;
                                    }
                            }
                        }
                    }

                }
                catch (Exception) { }
            }
            try
            {
                if (DT_HEAD.Rows[0][12].ToString().Trim() == "1")
                {
                    var MainPath = Server.MapPath("~/Drive/Hospitality/Rgfmcon/" + ViewBag.FormID);
                    if (System.IO.File.Exists(MainPath + "/" + "HEADSCR.txt") == true)
                    {
                        string readText = System.IO.File.ReadAllText(MainPath + "/" + "HEADSCR.txt");
                        @Html.Raw(HttpUtility.HtmlDecode(readText))
                    }
                }
            }
            catch (Exception) { }
        }
    }
