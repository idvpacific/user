}
<link rel="stylesheet" type="text/css" href="~/Theme/Base/app-assets/vendors/css/tables/datatable/datatables.min.css">
<link rel="stylesheet" type="text/css" href="~/Theme/Base/app-assets/vendors/css/extensions/sweetalert2.min.css">
<link rel="stylesheet" type="text/css" href="~/Theme/Base/app-assets/vendors/css/forms/select/select2.min.css">
<style>
    table.dataTable thead .sorting:before, table.dataTable thead .sorting_asc:before, table.dataTable thead .sorting_desc:before, table.dataTable thead .sorting:after, table.dataTable thead .sorting_asc:after, table.dataTable thead .sorting_desc:after {
        left: 10px
    }

    .MenuToolbox:after {
        content: '\2807';
        font-size: 22px;
    }

    tr:hover {
        background-color: rgba(235, 234, 234, 0.76) !important;
    }

    .lds-ripple {
        display: inline-block;
        position: relative;
        width: 80px;
        height: 80px;
    }

        .lds-ripple div {
            position: absolute;
            border: 4px solid #006abc;
            opacity: 1;
            border-radius: 50%;
            animation: lds-ripple 1s cubic-bezier(0, 0.2, 0.8, 1) infinite;
        }

            .lds-ripple div:nth-child(2) {
                animation-delay: -0.5s;
            }

    @@keyframes lds-ripple {
        0% {
            top: 36px;
            left: 36px;
            width: 0;
            height: 0;
            opacity: 1;
        }

        100% {
            top: 0px;
            left: 0px;
            width: 72px;
            height: 72px;
            opacity: 0;
        }
    }

    .sidepanel {
        width: 300px;
        position: fixed;
        z-index: 1;
        height: auto;
        top: 160px;
        right: -300px;
        background-color: #183851;
        overflow-x: hidden;
        transition: 0.5s;
        border-radius: 10px 0px 0px 10px;
        -webkit-box-shadow: 1px 9px 22px -4px rgba(0,0,0,0.71);
        box-shadow: 1px 9px 22px -4px rgba(0,0,0,0.71);
        opacity: 0.95;
        padding-bottom: 20px
    }

        .sidepanel .closebtn {
            position: absolute;
            top: 0;
            left: 10px;
            font-size: 20px;
            margin-bottom: 0px;
            padding: 0px
        }
</style>
<form id="mySidepanel" class="sidepanel">
    <div class="closebtn"><i class="fa fa-close" onclick="closeNav()" style="cursor:pointer"></i> <i class="fa fa-refresh" onclick="ResetNav()" style="cursor:pointer;font-size:16px"></i></div>
    <div style="padding-left:15px;padding-right:10px;margin-top:35px">
        <div style="margin-left:10px;color:white;min-width:200px;margin-bottom:5px">Application Status <span style="right:20px;position:absolute;cursor:pointer" onclick="SaveEdit('1')">[ <i class="fa fa-save"></i> Save ]</span></div>
        <select class="select2 form-control" id="Edit_S">
            <option value="1" @ViewBag.ST1>Pending</option>
            <option value="2" @ViewBag.ST2>Review</option>
            <option value="3" @ViewBag.ST3>Failed</option>
            <option value="4" @ViewBag.ST4>Passed</option>
        </select>
    </div>
    <div style="padding-left:20px;padding-right:10px;color:whitesmoke;margin-top:10px">
        @Html.Raw(ViewBag.ValiLog)
    </div>
    @if (ViewBag.ISGuesty == "1")
    {
        <hr style="background-color:darkgray" />
        <div style="padding-left:15px;padding-right:10px;margin-top:10px">
            <div style="margin-left:10px;color:white;min-width:200px;margin-bottom:5px">Guesty Status <span style="right:20px;position:absolute;cursor:pointer" onclick="SaveEdit('2')">[ <i class="fa fa-save"></i> Save ]</span></div>
            <select class="select2 form-control" id="Edit_G">
                <option value="1" selected>Approve</option>
