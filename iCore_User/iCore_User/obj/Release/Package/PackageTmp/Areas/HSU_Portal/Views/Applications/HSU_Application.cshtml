    <script src="~/Theme/Base/app-assets/vendors/js/forms/select/select2.full.min.js"></script>
    <script>
        $(".select2").select2({ dropdownAutoWidth: true, width: '100%' });
        function ShowRealSize(IMGCode) { var IMGC = document.getElementById(IMGCode); var modalImg = document.getElementById("IMGModal"); modalImg.src = IMGC.src; $("#IMGRSIMGMDL").modal(); }
        function CloseModal() { $("#IMGRSIMGMDL").modal("hide"); }

        function openNav() { document.getElementById("mySidepanel").style.right = "0px"; }
        function closeNav() { document.getElementById("mySidepanel").style.right = "-300px"; }
        function ResetNav() { document.getElementById("mySidepanel").reset(); $(".select2").select2({ dropdownAutoWidth: true, width: '100%' });}
        function SaveEdit(WID) {
            try {
                toastr.clear();
                toastr.info('The system is processing your request', 'Please wait ...', { "showMethod": "slideDown", "hideMethod": "slideUp", positionClass: 'toast-bottom-right', "progressBar": true, "closeButton": true, "timeOut": 8000 });
                var SID = "0"; var URL = "";
                if (WID == '1') { var SID = $("#Edit_S :selected").val().trim(); var URL = "@Url.Action("Application_ChangeStatus", "Applications", new { id="", area= "HSU_Portal" })"; } else { var SID = $("#Edit_G :selected").val().trim(); var URL = "@Url.Action("Application_ChangeGuesty", "Applications", new { id="", area= "HSU_Portal" })"; }
                var FrmDataPro = new FormData();
                FrmDataPro.append("AID", "@ViewBag.AID");
                FrmDataPro.append("SID", SID);
                $.ajax({
                    url: URL, type: "POST", data: FrmDataPro, dataType: 'json', contentType: false, processData: false, async: true,
                    error: function () {
                        toastr.clear();
                        toastr.error('An error occurred while processing your request', 'An error has occurred! [C02]', { "showMethod": "slideDown", "hideMethod": "slideUp", positionClass: 'toast-bottom-right', "progressBar": true, "closeButton": true, "timeOut": 8000 });
                    },
                    success: function (data)
                    {
                        var ErrorCode = ""; var ErrorResult = "";
                        $.each(data, function (i, item) { ErrorResult = item.Text; ErrorCode = item.Value; });
                        if (ErrorCode == "0")
                        {
                            if (WID == '1') {
                                location.reload();
                            }
                            else {
                                toastr.clear();
                                toastr.success(ErrorResult.trim(), 'Congratulations', { "showMethod": "slideDown", "hideMethod": "slideUp", positionClass: 'toast-bottom-right', "progressBar": true, "closeButton": true, "timeOut": 8000 });
                            }
                        }
                        else
                        {
                            if (ErrorCode != "2") {
                                toastr.clear();
                                toastr.error(ErrorResult.trim(), 'An error has occurred! [C03]', { "showMethod": "slideDown", "hideMethod": "slideUp", positionClass: 'toast-bottom-right', "progressBar": true, "closeButton": true, "timeOut": 8000 });
                            }
                            else { window.location.href = "@Url.Action("Logout", "Dashboard", new { id = "", area = "" })"; }
                        }
                    }
                });
            } catch (e) {
                toastr.clear();
                toastr.error('An error occurred while processing your request', 'An error has occurred! [C01]', { "showMethod": "slideDown", "hideMethod": "slideUp", positionClass: 'toast-bottom-right', "progressBar": true, "closeButton": true, "timeOut": 8000 });
            }
        }
    </script>
}